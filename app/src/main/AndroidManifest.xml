<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sohel.bookmyticket">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookingForm" />
        <activity android:name=".Movie1Activity" />
        <activity android:name=".Movie2Activity" />
        <activity android:name=".Movie3Activity" />
        <activity android:name=".Movie4Activity" />
        <activity android:name=".Movie5Activity" />
        <activity android:name=".Movie6Activity" />
    </application>

</manifest>